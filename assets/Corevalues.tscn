theme_override_colors/default_color = Color(0, 0, 0, 1)
theme_override_fonts/normal_font = ExtResource("2_ipvhi")
theme_override_font_sizes/normal_font_size = 16
text = "The beginnings of Partido High School went back to the time when the Provincial Board of Camarines Sur announced that whichever town that could meet the basic requirements of a school site, an enrolment of at least 200 students, and a building that could house the classes, a branch of Camarines Sur National High School (in Naga City) would be established. 
Goa mobilized its whole community. A school site was secured – a riceland located at the boundary of Goa – San Jose.

In June 1941, while war threats circulated, the Partido High School opened its first classes with a little over 200 first year students in four sections. They came from different towns in Goa Partido, representing all social classes."
metadata/_edit_use_anchors_ = true

[node name="Page 2" type="RichTextLabel" parent="Panel"]
layout_mode = 1
anchors_preset = -1
anchor_left = 0.5130208
anchor_top = 0.15925926
anchor_right = 0.8520833
anchor_bottom = 0.80277777
